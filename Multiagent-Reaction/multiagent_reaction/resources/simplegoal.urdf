<?xml version="1.0"?> 
<robot name="simplegoal">
    <!-- Colors --> 
    <material name="red">
        <color rgba="255 0 0 1"/>
    </material>

    <!-- Goal -->
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="1" length="1"/>
                <origin rpy="0 0 0" xyz="0 0 0"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision> 
             <geometry> 
                <cylinder radius="0.25" length="0.2"/>
                <origin rpy="0 0 0" xyz="0 0 0"/>
            </geometry>
        </collision> 
        <inertial> 
            <mass value="4"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial> 
    </link>
</robot>
